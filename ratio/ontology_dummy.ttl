                                        rdf:first "Female" ;
                                        rdf:rest [ rdf:type rdf:List ;
                                                   rdf:first "Male" ;
                                                   rdf:rest [ rdf:type rdf:List ;
                                                              rdf:first "Mixed" ;
                                                              rdf:rest rdf:nil
                                                            ]
                                                 ]
