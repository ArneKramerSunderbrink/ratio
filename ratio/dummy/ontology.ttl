@prefix : <http://www.semanticweb.org/root/ontologies/2018/6/ctro#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ctro: <http://www.semanticweb.org/root/ontologies/2018/6/ctro#> .
@prefix ratio: <http://www.example.org/ratio-tool#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

### Base for new URIs created in the tool
ratio:Configuration ratio:hasBase "http://www.semanticweb.org/root/ontologies/2018/6/ctro#" .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2006/time#Interval
<http://www.w3.org/2006/time#Interval> rdf:type rdfs:Datatype .


#################################################################
#    Subheadings
#################################################################

ctro:hasStatisticalMeasure rdf:type ratio:Subheading ;
                           ratio:order "10"^^xsd:positiveInteger ;
                           rdfs:domain ctro:Outcome ;
                           rdfs:label "Statistical measurements:" .

ctro:hasStatisticalMeasureAE rdf:type ratio:Subheading ;
                             ratio:order "5"^^xsd:positiveInteger ;
                             rdfs:domain ctro:AdverseEffect ;
                             rdfs:label "Statistical measurements:" .

#################################################################
#    Object Properties
#################################################################

ctro:described_by rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  ratio:described "true"^^xsd:boolean ;
                  ratio:deletable "false"^^xsd:boolean ;
                  ratio:order "1"^^xsd:positiveInteger ;
                  rdfs:domain ctro:ClinicalTrial ;
                  rdfs:range ctro:Publication ;
                  rdfs:label "is described by Publication" .

ctro:hasDesignGroup rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                    ratio:described "true"^^xsd:boolean ;
                    ratio:deletable "false"^^xsd:boolean ;
                    ratio:order "3"^^xsd:positiveInteger ;
                    rdfs:domain ctro:ClinicalTrial ;
                    rdfs:range ctro:DesignGroup ;
                    rdfs:label "Clinical trial design" .

ctro:hasCTDesign rdf:type owl:ObjectProperty ;
                 ratio:order "5"^^xsd:positiveInteger ;
                 rdfs:domain ctro:DesignGroup ;
                 rdfs:range ctro:CTDesign ;
                 rdfs:label "Clinical trial design" .

ctro:analysesHealthCondition rdf:type owl:ObjectProperty ;
                             ratio:order "1"^^xsd:positiveInteger ;
                             rdfs:domain ctro:DesignGroup ;
                             rdfs:range ctro:DisorderOrSyndrome ;
                             rdfs:label "Disease" .

ctro:hasAnalysisApproach rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         ratio:order "4"^^xsd:positiveInteger ;
                         rdfs:domain ctro:DesignGroup ;
                         rdfs:range ctro:CTAnalysisApproach ;
                         rdfs:label "Analysis approach" .

ctro:hasPopulation rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   ratio:described "true"^^xsd:boolean ;
                   ratio:deletable "false"^^xsd:boolean ;
                   ratio:order "4"^^xsd:positiveInteger ;
                   rdfs:domain ctro:ClinicalTrial ;
                   rdfs:range ctro:Population ;
                   rdfs:label "Population" .

ctro:hasGender rdf:type owl:ObjectProperty ;
               ratio:order "1"^^xsd:positiveInteger ;
               ratio:width "25"^^xsd:positiveInteger ;
               rdfs:domain ctro:Population ;
               rdfs:range ctro:Gender ;
               rdfs:label "Gender" .

ctro:hasEthnicity rdf:type owl:ObjectProperty ;
                  ratio:order "7"^^xsd:positiveInteger ;
                  ratio:width "25"^^xsd:positiveInteger ;
                  rdfs:domain ctro:Population ;
                  rdfs:range ctro:Ethnicity ;
                  rdfs:label "Ethnicity" .

ctro:hasCountry rdf:type owl:ObjectProperty ;
                ratio:order "8"^^xsd:positiveInteger ;
                ratio:width "25"^^xsd:positiveInteger ;
                rdfs:domain ctro:Population ;
                rdfs:range <http://www.bpiresearch.com/BPMO/2004/03/03/cdl/Countries#Country> ;
                rdfs:label "Country" .

ctro:hasQualityIndicator rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty;
                         ratio:described "true"^^xsd:boolean ;
                         ratio:deletable "false"^^xsd:boolean ;
                         ratio:order "11"^^xsd:positiveInteger ;
                         rdfs:domain ctro:ClinicalTrial ;
                         rdfs:range ctro:EvidenceQuality ;
                         rdfs:label "Potential Conflict of Interest" .

ctro:hasArm rdf:type owl:ObjectProperty ;
            ratio:described "true"^^xsd:boolean ;
            ratio:order "5"^^xsd:positiveInteger ;
            rdfs:domain ctro:ClinicalTrial ;
            rdfs:range ctro:Arm ;
            rdfs:label "Interventional-Arm" .

ctro:hasIntervention rdf:type owl:ObjectProperty ;
                     ratio:described "true"^^xsd:boolean ;
                     ratio:order "2"^^xsd:positiveInteger ;
                     rdfs:domain ctro:Arm ;
                     rdfs:range ctro:Intervention ;
                     rdfs:label "Intervention" .

ctro:hasFreqInterval rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  ratio:order "2"^^xsd:positiveInteger ;
                  ratio:width "25"^^xsd:positiveInteger ;
                  rdfs:domain ctro:Intervention ;
                  rdfs:range ctro:FrequencyInterval ;
                  rdfs:label "Frequency Interval" .

ctro:hasDrug rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             ratio:order "3"^^xsd:positiveInteger ;
             ratio:width "25"^^xsd:positiveInteger ;
             rdfs:domain ctro:Intervention ;
             rdfs:range ctro:Drug ;
             rdfs:label "Drug" .

ctro:hasDoseUnit rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 ratio:order "5"^^xsd:positiveInteger ;
                 ratio:width "25"^^xsd:positiveInteger ;
                 rdfs:domain ctro:Intervention ;
                 rdfs:range ctro:BioAndMedicalUnit ;
                 rdfs:label "Dose unit" .

ctro:hasDeliveryMethod rdf:type owl:ObjectProperty ;
                       ratio:order "6"^^xsd:positiveInteger ;
                       rdfs:domain ctro:Intervention ;
                       rdfs:range ctro:DeliveryMethod ;
                       rdfs:label "Delivery method" .

ctro:hasEndPoint rdf:type owl:ObjectProperty ;
                 ratio:described "true"^^xsd:boolean ;
                 ratio:order "6"^^xsd:positiveInteger ;
                 rdfs:domain ctro:ClinicalTrial ;
                 rdfs:range ctro:EndPoint ;
                 rdfs:label "Endpoint" .

ctro:hasEndPointOut rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    ratio:order "2"^^xsd:positiveInteger ;
                    rdfs:domain ctro:Outcome ;
                    rdfs:range ctro:EndPoint ;
                    rdfs:label "Endpoint" .

ctro:hasEndpointDescription rdf:type owl:ObjectProperty ;
                            ratio:order "1"^^xsd:positiveInteger ;
                            rdfs:subPropertyOf owl:topObjectProperty ;
                            rdf:type owl:FunctionalProperty ;
                            rdfs:domain ctro:EndPoint ;
                            rdfs:range ctro:EndpointDesc_AF ;
                            rdfs:label "Outcome Measure" .

ctro:hasAggregationMethod rdf:type owl:ObjectProperty ,
                                   owl:FunctionalProperty ;
                          ratio:order "4"^^xsd:positiveInteger ;
                          rdfs:domain ctro:EndPoint ;
                          rdfs:range ctro:AgreggationMethod ;
                          rdfs:label "Aggregation method" .

ctro:hasEndpointUnit rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     ratio:order "6"^^xsd:positiveInteger ;
                     ratio:width "25"^^xsd:positiveInteger ;
                     rdfs:domain ctro:EndPoint ;
                     rdfs:range ctro:BioAndMedicalUnit ;
                     rdfs:label "Endpoint unit" .

ctro:hasAnalysisMetric rdf:type owl:ObjectProperty ,
                                owl:FunctionalProperty ;
                       ratio:order "7"^^xsd:positiveInteger ;
                       rdfs:domain ctro:EndPoint ;
                       rdfs:range ctro:AnalysisMetric ;
                       rdfs:label "Analysis metric" .

# in the original ontology adverse effects and outcomes both have result children
# currently in the tool outcomes don't have (described) children, instead outcomes and effects are siblings
# and the outcome is referenced in an option-field of the result, while adverse events don't have effects at all,
# just some of the fields an effect would have

ctro:hasOutcome rdf:type owl:ObjectProperty ;
               ratio:described "true"^^xsd:boolean ;
               ratio:order "7"^^xsd:positiveInteger ;
               rdfs:domain ctro:ClinicalTrial ;
               rdfs:range ctro:Outcome ;
               rdfs:label "Outcome Measure Data" .

ctro:hasDiffBetweenGroups rdf:type owl:ObjectProperty ;
                          ratio:described "true"^^xsd:boolean ;
                          ratio:order "8"^^xsd:positiveInteger ;
                          rdfs:domain ctro:ClinicalTrial ;
                          rdfs:range ctro:DiffBetweenGroups ;
                          rdfs:label "Difference between groups" .

ctro:hasOutcome1 rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 ratio:order "1"^^xsd:positiveInteger ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain ctro:DiffBetweenGroups ;
                 rdfs:range ctro:Outcome ;
                 rdfs:label "Outcome of group 1" .


ctro:hasOutcome2 rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 ratio:order "2"^^xsd:positiveInteger ;
                 rdfs:domain ctro:DiffBetweenGroups ;
                 rdfs:range ctro:Outcome ;
                 rdfs:label "Outcome of group 2" .

ctro:hasMeasureName rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    ratio:order "3"^^xsd:positiveInteger ;
                    rdfs:domain ctro:DiffBetweenGroups ;
                    rdfs:range ctro:DiffGroupsMeasureName ;
                    rdfs:label "Measure" .

ctro:hasArmOut rdf:type owl:ObjectProperty ;
               ratio:order "1"^^xsd:positiveInteger ;
               rdfs:domain ctro:Outcome ;
               rdfs:range ctro:Arm ;
               rdfs:label "Interventional-Arm" .

ctro:hasMeasurementDevice rdf:type owl:ObjectProperty ;
                          ratio:order "8"^^xsd:positiveInteger ;
                          rdfs:domain ctro:EndPoint ;
                          rdfs:range ctro:MeasurementDevice ;
                          rdfs:label "Measurement device" .

ctro:hasAdverseEffect rdf:type owl:ObjectProperty ;
                      ratio:described "true"^^xsd:boolean ;
                      ratio:order "9"^^xsd:positiveInteger ;
                      rdfs:domain ctro:ClinicalTrial ;
                      rdfs:range ctro:AdverseEffect ;
                      rdfs:label "Adverse Event" .

ctro:hasArmAE rdf:type owl:ObjectProperty ;
              ratio:order "1"^^xsd:positiveInteger ;
              rdfs:domain ctro:AdverseEffect ;
              rdfs:range ctro:Arm ;
              rdfs:label "Interventional-Arm" .

ctro:hasAEname rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               ratio:order "2"^^xsd:positiveInteger ;
               rdfs:domain ctro:AdverseEffect ;
               rdfs:range ctro:EndpointDesc_AF ;
               rdfs:label "Adverse effect description" .

#################################################################
#    Data properties
#################################################################

ctro:hasObjectiveDescription rdf:type owl:DatatypeProperty ,
                                      owl:FunctionalProperty ;
                             ratio:order "2"^^xsd:positiveInteger ;
                             ratio:width "100"^^xsd:positiveInteger ;
                             rdfs:domain ctro:ClinicalTrial ;
                             rdfs:range xsd:string ;
                             rdfs:label "Objective" .

ctro:hasTitle rdf:type owl:DatatypeProperty ,
