                             rdfs:domain ctro:AdverseEffect ;
                             rdfs:range xsd:string ;
                             rdfs:label "Confidence Interval" .

ctro:hasDiffGroupAbsValue rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
