                   rdfs:range xsd:string ;
                   rdfs:label "P-value" .

ctro:hasNumberAffectedAE rdf:type owl:DatatypeProperty ,
                                  owl:FunctionalProperty ;
                         ratio:order "3"^^xsd:positiveInteger ;
                         rdfs:domain ctro:AdverseEffect ;
                         rdfs:range xsd:integer ;
                         rdfs:label "Number of patients affected" .

ctro:hasObservedResultAE rdf:type owl:DatatypeProperty ,
                                  owl:FunctionalProperty ;
                         ratio:order "4"^^xsd:positiveInteger ;
