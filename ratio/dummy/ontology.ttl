                         ratio:width "25"^^xsd:positiveInteger ;
                         rdfs:domain ctro:DesignGroup ;
                         rdfs:range xsd:positiveInteger ;
                         rdfs:label "Initial number of patients" .

ctro:hasCTduration rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   ratio:order "3"^^xsd:positiveInteger ;
                   ratio:width "25"^^xsd:positiveInteger ;
                   rdfs:domain ctro:DesignGroup ;
                   rdfs:range xsd:positiveInteger ;
                   rdfs:comment "Duration of the trial given in WEEKS" ;
                   rdfs:label "Trial duration (in weeks)" .

ctro:hasMinAge rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               ratio:order "2"^^xsd:positiveInteger ;
               ratio:width "25"^^xsd:positiveInteger ;
               rdfs:domain ctro:Population ;
               rdfs:range xsd:positiveInteger ;
               rdfs:label "Min. age" .

ctro:hasMaxAge rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               ratio:order "3"^^xsd:positiveInteger ;
               ratio:width "25"^^xsd:positiveInteger ;
               rdfs:domain ctro:Population ;
               rdfs:range xsd:positiveInteger ;
               rdfs:label "Max. age" .

ctro:hasAvgAge rdf:type owl:DatatypeProperty ;
               ratio:order "4"^^xsd:positiveInteger ;
               ratio:width "25"^^xsd:positiveInteger ;
               rdfs:domain ctro:Population ;
               rdfs:range xsd:nonNegativeInteger ;
               rdfs:label "Avg Age" .

ctro:hasPreconditionDescription rdf:type owl:DatatypeProperty ;
                                ratio:order "5"^^xsd:positiveInteger ;
                                ratio:width "100"^^xsd:positiveInteger ;
                                rdfs:domain ctro:Population ;
                                rdfs:range xsd:string ;
                                rdfs:label "Precondition" .

ctro:hasExclusionCriteria rdf:type owl:DatatypeProperty ;
                          ratio:order "6"^^xsd:positiveInteger ;
                          ratio:width "100"^^xsd:positiveInteger ;
                          rdfs:domain ctro:Population ;
                          rdfs:range rdfs:Literal ;
                          rdfs:label "Exclusion criteria" .

ctro:hasAuthorAffiliation rdf:type owl:DatatypeProperty ;
                         ratio:order "2"^^xsd:positiveInteger ;
                         rdfs:domain ctro:EvidenceQuality ;
                         rdfs:range xsd:string ;
                         rdfs:label "Author affiliation" .

ctro:hasConflictInterest rdf:type owl:DatatypeProperty ;
                         ratio:order "3"^^xsd:positiveInteger ;
                         ratio:width "100"^^xsd:positiveInteger ;
                         rdfs:domain ctro:EvidenceQuality ;
                         rdfs:range xsd:string ;
                         rdfs:label "Conflict of interest" .

ctro:hasSponsor rdf:type owl:DatatypeProperty ;
                ratio:order "1"^^xsd:positiveInteger ;
                rdfs:domain ctro:EvidenceQuality ;
                rdfs:range xsd:string ;
                rdfs:label "Sponsor" .

ctro:hasNumberPatientsArm rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          ratio:order "1"^^xsd:positiveInteger ;
                          rdfs:domain ctro:Arm ;
                          rdfs:range xsd:positiveInteger ;
                          rdfs:label "Number of patients in arm" .

ctro:hasDuration rdf:type owl:DatatypeProperty ;
                 ratio:order "1"^^xsd:positiveInteger ;
                 rdfs:domain ctro:Intervention ;
                 rdfs:range rdfs:Literal ;
                 rdfs:label "Intervention duration" ;
                 rdfs:comment "Duration of the intervention given in WEEKS" .

ctro:hasDoseValue rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  ratio:order "4"^^xsd:positiveInteger ;
                  ratio:width "75"^^xsd:positiveInteger ;
                  rdfs:domain ctro:Intervention ;
                  rdfs:range rdfs:Literal ;
                  rdfs:label "Dose value or description" .

ctro:isControl rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               ratio:order "7"^^xsd:positiveInteger ;
               ratio:width "25"^^xsd:positiveInteger ;
               rdfs:domain ctro:Intervention ;
               rdfs:range xsd:boolean ;
               rdfs:label "Control or placebo" .

ctro:hasBaselineValue rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      ratio:order "5"^^xsd:positiveInteger ;
                      ratio:width "25"^^xsd:positiveInteger ;
                      rdfs:domain ctro:EndPoint ;
                      rdfs:range xsd:float ;
                      rdfs:label "Baseline value" .

ctro:hasEndPointDescriptionStr rdf:type owl:DatatypeProperty ,
                                        owl:FunctionalProperty ;
                               ratio:order "3"^^xsd:positiveInteger ;
                               ratio:width "100"^^xsd:positiveInteger ;
                               rdfs:domain ctro:EndPoint ;
                               rdfs:range xsd:string ;
                               rdfs:label "Endpoint description" .

ctro:hasOutcomeType rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    ratio:order "2"^^xsd:positiveInteger ;
                    rdfs:domain ctro:EndPoint ;
                    rdfs:range [ rdf:type rdfs:Datatype ;
                                 owl:oneOf [ rdf:type rdf:List ;
                                             rdf:first "Primary" ;
                                             rdf:rest [ rdf:type rdf:List ;
                                                        rdf:first "Secondary" ;
                                                        rdf:rest rdf:nil
                                                      ]
                                           ]
                               ] ;
                    rdfs:label "Outcome type" .

ctro:hasFinalNumberPatients rdf:type owl:DatatypeProperty ,
                                     owl:FunctionalProperty ;
                            ratio:order "3"^^xsd:positiveInteger ;
                            ratio:width "25"^^xsd:positiveInteger ;
                            rdfs:domain ctro:Outcome ;
                            rdfs:range xsd:positiveInteger ;
                            rdfs:label "Final number of patients" .

ctro:hasTimePoints rdf:type owl:DatatypeProperty ;
                   rdfs:domain ctro:Outcome ;
                   ratio:order "4"^^xsd:positiveInteger ;
                   ratio:width "25"^^xsd:positiveInteger ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Point of time when an outcome was measured. E.g. study end, after two weeks, etc." ;
                   rdfs:label "Time point" .

ctro:hasNumberAffected rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       ratio:order "5"^^xsd:positiveInteger ;
                       rdfs:domain ctro:Outcome ;
                       rdfs:range xsd:integer ;
                       rdfs:label "Number of patients affected" .

ctro:hasAbsoluteValue rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      ratio:order "6"^^xsd:positiveInteger ;
                      ratio:width "25"^^xsd:positiveInteger ;
                      rdfs:domain ctro:Outcome ;
                      rdfs:range xsd:float ;
                      rdfs:label "Value" .

ctro:hasRelativeValue rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      ratio:order "7"^^xsd:positiveInteger ;
                      ratio:width "25"^^xsd:positiveInteger ;
                      rdfs:domain ctro:Outcome ;
                      rdfs:range xsd:float ;
                      rdfs:label "Relative value" .

ctro:hasResultDirection rdf:type owl:DatatypeProperty ,
                                  owl:FunctionalProperty ;
                         ratio:order "8"^^xsd:positiveInteger ;
                         rdfs:domain ctro:Outcome ;
                         rdfs:range [ rdf:type rdfs:Datatype ;
                                      owl:oneOf [ rdf:type rdf:List ;
                                                  rdf:first "Increment" ;
                                                  rdf:rest [ rdf:type rdf:List ;
                                                             rdf:first "No change" ;
                                                             rdf:rest [ rdf:type rdf:List ;
                                                                        rdf:first "Reduction" ;
                                                                        rdf:rest rdf:nil
                                                                      ]
                                                           ]
                                                ]
                                    ] ;
                         rdfs:comment "Direction of the change from baseline: reduction, increment, No change" ;
                         rdfs:label "Direction of the change" .

ctro:hasObservedResult rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       ratio:order "9"^^xsd:positiveInteger ;
                       ratio:width "100"^^xsd:positiveInteger ;
                       rdfs:domain ctro:Outcome ;
                       rdfs:range xsd:string ;
                       rdfs:label "Observed results" .

ctro:hasStatisticalTestName rdf:type owl:DatatypeProperty ,
                                     owl:FunctionalProperty ;
                            ratio:order "11"^^xsd:positiveInteger ;
                            rdfs:domain ctro:Outcome ;
                            rdfs:range xsd:string ;
                            rdfs:label "Statistical test name" .

ctro:hasStatisticalTestNameAE rdf:type owl:DatatypeProperty ,
