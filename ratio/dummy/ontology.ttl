                          ratio:width "25"^^xsd:positiveInteger ;
                          rdfs:domain ctro:DiffBetweenGroups ;
                          rdfs:range xsd:float ;
                          rdfs:label "Absolute value" .


ctro:hasDiffGroupRelValue rdf:type owl:DatatypeProperty ;
                          ratio:order "5"^^xsd:positiveInteger ;
                          ratio:width "25"^^xsd:positiveInteger ;
                          rdfs:domain ctro:DiffBetweenGroups ;
                          rdfs:range xsd:float ;
                          rdfs:label "Relative value" .

ctro:hasStandardDevDiff rdf:type owl:DatatypeProperty ,
                                 owl:FunctionalProperty ;
                        ratio:order "6"^^xsd:positiveInteger ;
                        rdfs:domain ctro:DiffBetweenGroups ;
                        rdfs:range xsd:float ;
                        rdfs:label "Standard deviation" .

ctro:hasStandardErrorDiff rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          ratio:order "7"^^xsd:positiveInteger ;
                          rdfs:domain ctro:DiffBetweenGroups ;
                          rdfs:range xsd:float ;
                          rdfs:label "Standard error" .

ctro:hasConfIntervalDiff rdf:type owl:DatatypeProperty ,
