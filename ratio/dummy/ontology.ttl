ctro:hasStatisticalTestValueAE rdf:type owl:DatatypeProperty ,
                                        owl:FunctionalProperty ;
                               ratio:order "7"^^xsd:positiveInteger ;
                               ratio:width "25"^^xsd:positiveInteger ;
                               rdfs:domain ctro:AdverseEffect ;
                               rdfs:range xsd:float ;
                               rdfs:label "Statistical test value" .

ctro:hasPValue rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               ratio:order "13"^^xsd:positiveInteger ;
               ratio:width "25"^^xsd:positiveInteger ;
               rdfs:domain ctro:Outcome ;
               rdfs:range xsd:string ;
               rdfs:label "P-value" .

ctro:hasPValueAE rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 ratio:order "8"^^xsd:positiveInteger ;
                 ratio:width "25"^^xsd:positiveInteger ;
                 rdfs:domain ctro:AdverseEffect ;
                 rdfs:range xsd:string ;
                 rdfs:label "P-value" .

ctro:hasStandardDev rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    ratio:order "14"^^xsd:positiveInteger ;
                    ratio:width "25"^^xsd:positiveInteger ;
                    rdfs:domain ctro:Outcome ;
                    rdfs:range xsd:float ;
                    rdfs:label "Standard Deviation" .

ctro:hasStandardDevAE rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      ratio:order "9"^^xsd:positiveInteger ;
                      ratio:width "25"^^xsd:positiveInteger ;
                      rdfs:domain ctro:AdverseEffect ;
                      rdfs:range xsd:float ;
                      rdfs:label "Standard Deviation" .

ctro:hasConfidenceInterval rdf:type owl:DatatypeProperty,
                                    owl:FunctionalProperty ;
                           ratio:order "15"^^xsd:positiveInteger ;
                           rdfs:domain ctro:Outcome ;
                           rdfs:range xsd:string ;
                           rdfs:label "Confidence Interval" .

ctro:hasConfidenceIntervalAE rdf:type owl:DatatypeProperty,
